<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Extends the ScenarioContext to allow for enums as keys instead of strings</Description>
    <PackageProjectUrl>https://github.com/RasicN/specflow.scenariocontext.enum</PackageProjectUrl>
    <RepositoryUrl>https://github.com/RasicN/specflow.scenariocontext.enum</RepositoryUrl>
    <PackageReleaseNotes>Initial version</PackageReleaseNotes>
    <Authors>Joe Cisar</Authors>
    <Product />
    <PackageId>SpecflowExtensions.ScenarioContext.Enum</PackageId>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="SpecFlow" Version="3.0.225" />
  </ItemGroup>

</Project>
